---
layout: null
---
[
  {% for text in site.texts %}
  {
    "title": {{ text.title | jsonify }},
    "author": {{ text.author_name | jsonify }},
    "url": {{ text.url | jsonify }},
    "lang": {{ text.lang | jsonify }},
    "date": {{ text.date | date: "%Y" | jsonify }},
    "source": {{ text.source | default: "" | jsonify }},
    "country": {{ text.country | default: "" | jsonify }},
    "collections": {{ text.collections | default: "" | jsonify }}
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
